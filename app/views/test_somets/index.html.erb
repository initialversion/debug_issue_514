<div class="page-header">
  <h1>
    Test somets
    <a href="/test_somets/new" class="btn btn-lg btn-success">New Test somet</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Etc</th>
        <th>Actions</th>
      </tr>

      <% @test_somets.each do |test_somet| %>
      <tr>
        <td><%= test_somet.name %></td>
        <td><%= test_somet.etc %></td>
        <td>
          <a href="/test_somets/<%= test_somet.id %>" class="btn btn-primary">Show</a>
          <a href="/test_somets/<%= test_somet.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_test_somet/<%= test_somet.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
